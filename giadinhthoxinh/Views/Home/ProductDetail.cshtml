<style>
    .product-container {
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: #ffffff;
        border: 1px solid #e0e0e0;
        border-radius: 10px;
        padding: 15px;
        overflow: hidden;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s, box-shadow 0.3s;
    }

        .product-container:hover {
            transform: translateY(-5px);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
        }

        .product-container img {
            max-width: 100%;
            max-height: 300px;
            object-fit: cover;
            border-radius: 10px;
            transition: transform 0.3s;
        }

            .product-container img:hover {
                transform: scale(1.05);
            }

    .quantity .btn {
        border-radius: 50%;
        width: 30px;
        height: 30px;
        display: flex;
        align-items: center;
        justify-content: center;
        background-color: #f8f9fa;
        border: none;
        color: #495057;
    }

    .quantity input {
        max-width: 50px;
        border: 1px solid #dee2e6;
    }

    .nav-tabs .nav-link {
        color: #495057;
        background-color: #f8f9fa;
        border: 1px solid #dee2e6;
        border-radius: 5px 5px 0 0;
        margin-right: 5px;
    }

        .nav-tabs .nav-link.active {
            color: #ffffff;
            background-color: #007bff;
            border-color: #007bff;
        }

    .tab-content {
        background-color: #ffffff;
        border: 1px solid #dee2e6;
        border-top: none;
        padding: 20px;
        border-radius: 0 0 5px 5px;
    }

    .category-list {
        list-style: none;
        padding: 0;
    }

        .category-list li {
            margin-bottom: 10px;
        }

        .category-list a {
            text-decoration: none;
            color: #007bff;
            font-weight: bold;
        }

            .category-list a:hover {
                text-decoration: underline;
            }

    .featured-product img {
        max-height: 50px;
        object-fit: cover;
        margin-right: 10px;
        border-radius: 5px;
    }

    .featured-product h6 {
        margin: 0;
        font-weight: bold;
    }

    .btn-custom {
        border-radius: 50px;
        background-color: #007bff;
        color: #ffffff;
        transition: background-color 0.3s ease-in-out;
    }

        .btn-custom:hover {
            background-color: #0056b3;
        }

    .banner img {
        max-height: 200px;
        object-fit: cover;
        border-radius: 10px;
    }

    .banner .overlay {
        position: absolute;
        top: 50%;
        right: 10px;
        transform: translateY(-50%);
        background: rgba(255, 255, 255, 0.9);
        padding: 20px;
        border-radius: 10px;
    }

        .banner .overlay h3 {
            margin: 0;
            font-weight: bold;
            color: #007bff;
        }


    .product-container {
        background-color: #ffffff;
        border: 1px solid #e0e0e0;
        border-radius: 10px;
        overflow: hidden;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s, box-shadow 0.3s;
        display: flex;
        flex-direction: column;
    }

        .product-container:hover {
            transform: translateY(-5px);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
        }

        .product-container img {
            max-width: 100%;
            height: auto;
            transition: transform 0.3s;
        }

            .product-container img:hover {
                transform: scale(1.05);
            }

        .product-container h5 {
            margin-top: 10px;
            font-weight: 700;
            color: #333;
        }

        .product-container .fw-bold {
            color: #333;
            margin: 10px 0;
        }

        .product-container p {
            color: #777;
        }

        .product-container .btn-custom {
            background-color: #007bff;
            color: #fff;
            border: none;
            border-radius: 20px;
            padding: 5px 15px;
            margin-left:40px;
            transition: background-color 0.3s, transform 0.3s;
        }

            .product-container .btn-custom:hover {
                background-color: #0056b3;
                transform: translateY(-2px);
            }

        .product-container .p-3 {
            flex-grow: 1;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
        }
</style>

